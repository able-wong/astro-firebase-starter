# Application Configuration
# Copy this file to .env and fill in your values

# Firebase Client Configuration (PUBLIC - safe to expose in browser)
# Get this from Firebase Console > Project Settings > Your apps > Web app
PUBLIC_FIREBASE_CONFIG={"apiKey":"your-api-key","authDomain":"your-auth-domain","projectId":"your-project-id","storageBucket":"your-storage-bucket","messagingSenderId":"your-messaging-sender-id","appId":"your-app-id"}

# Firebase Admin SDK Configuration (for utility scripts only - NOT exposed to browser)
# Required for: import-firestore-data.js, firebase-data-fetcher.js, test-firebase-config.js
# Get this from Firebase Console > Project Settings > Service Accounts > Generate new private key
FIREBASE_PROJECT_ID="your-project-id"
FIREBASE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"...","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"..."}
